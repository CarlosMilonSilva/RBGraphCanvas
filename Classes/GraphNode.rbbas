#tag ClassProtected Class GraphNode	#tag Method, Flags = &h0		Function composedLabel() As picture		  dim tmp as picture		  dim w as integer		  		  if composed=nil then		    tmp=newpicture(2,2,16)		    tmp.graphics.textsize=10		    tmp.graphics.textfont="Arial"		    w=tmp.graphics.stringwidth(labelx+", "+labely)		    		    composed=newpicture(w+4,tmp.graphics.textascent+4,32)		    '255,255,230		    composed.graphics.foreColor=rgb(255,255,230)		    composed.graphics.fillrect 0,0,composed.width,composed.height		    composed.graphics.foreColor=&c000000		    composed.graphics.textfont="Arial"		    composed.graphics.textsize=10		    composed.graphics.drawstring labelx+", "+labely,2,composed.height-2		    composed.graphics.drawRect 0,0,composed.width,composed.height		  end if		  		  return composed		End Function	#tag EndMethod	#tag Method, Flags = &h0		Sub Constructor(labelx as string, labely as string, x as integer, y as integer, w as integer, col as color)		  self.labelx=labelx		  self.labely=labely		  self.x=x		  self.y=y		  self.w=w		  self.col=col		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Function over(ox as integer, oy as integer) As boolean		  isOver=(ox>=x and ox<=x+w and oy>=y and oy<=y+w)		  return isOver		End Function	#tag EndMethod	#tag Method, Flags = &h0		Sub paint(g as graphics)		  'Return		  		  g.foreColor=col		  g.fillOval x,y,w,w		  g.foreColor=&c000000		  g.drawOval x,y,w,w		End Sub	#tag EndMethod	#tag Property, Flags = &h1		Protected col As color	#tag EndProperty	#tag Property, Flags = &h1		Protected composed As picture	#tag EndProperty	#tag Property, Flags = &h1		Protected isOver As boolean	#tag EndProperty	#tag Property, Flags = &h1		Protected labelx As string	#tag EndProperty	#tag Property, Flags = &h1		Protected labely As string	#tag EndProperty	#tag Property, Flags = &h1		Protected w As integer	#tag EndProperty	#tag Property, Flags = &h1		Protected x As integer	#tag EndProperty	#tag Property, Flags = &h1		Protected y As integer	#tag EndProperty	#tag ViewBehavior		#tag ViewProperty			Name="Index"			Visible=true			Group="ID"			InitialValue="-2147483648"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Left"			Visible=true			Group="Position"			InitialValue="0"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Name"			Visible=true			Group="ID"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Super"			Visible=true			Group="ID"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Top"			Visible=true			Group="Position"			InitialValue="0"			InheritedFrom="Object"		#tag EndViewProperty	#tag EndViewBehaviorEnd Class#tag EndClass